#! /usr/bin/python
import argparse
import logging
import time


class App(object):

  def Run(self, args):
    now = int(time.time())
    print '%s.%s' % (now >> 16, now & 0xffff)


def main():
  parser = argparse.ArgumentParser(description='')
  parser.add_argument('--debug', action='store_true')
  args = parser.parse_args()
  if args.debug:
    logging.basicConfig(level=logging.DEBUG)
  App().Run(args)


if __name__ == '__main__': main()
